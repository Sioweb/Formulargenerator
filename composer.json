{
    "name": "sioweb/formgenerator",
    "description": "Use config json/array to build form",
    "type": "library",
    "authors": [
        {
            "name": "Sascha Weidner",
            "email": "support@sioweb.de"
        }
    ],
    "keywords": [
        "library",
        "form",
        "generator"
    ],
    "homepage": "https://github.com/sioweb/Formulargenerator",
    "license": "GPL-3.0-only",
    "require": {
        "sioweb/ccevent-git": "*"
    },
    "autoload": {
        "psr-4": {
            "Sioweb\\Lib\\Formgenerator\\": "./src/"
        }
    },
    "scripts": {
        "package-scripts": [
            "@config.localhost==1",
            "Sioweb\\CCEvent\\Git\\Composer\\Git::init --repository=https://github.com/Sioweb/Formulargenerator"
        ]
    }
}
